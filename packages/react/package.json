{
  "name": "@siggn/react",
  "version": "0.0.1",
  "description": "A lightweight type safe message bus system for React",
  "keywords": [
    "pub/sub",
    "message",
    "event",
    "bus",
    "react",
    "typesafe"
  ],
  "publishConfig": {
    "@siggn:registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://github.com/Guiguerreiro39/siggn",
  "bugs": {
    "url": "https://github.com/Guiguerreiro39/siggn/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Guiguerreiro39/siggn.git/",
    "directory": "packages/react"
  },
  "license": "MIT",
  "author": "Guilherme Guerreiro (https://guilhermegr.com)",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "test:watch": "vitest",
    "test": "vitest run",
    "build": "vite build",
    "prepack": "pnpm run build"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "@vitejs/plugin-react": "^5.1.0",
    "vitest": "^3.2.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.15",
    "react": "^19.1.1",
    "jsdom": "^27.0.1"
  },
  "peerDependencies": {
    "typescript": "^5",
    "react": ">=18 <20"
  },
  "dependencies": {
    "@siggn/core": "workspace:*"
  }
}
